{
  "manifest_version": 2,

  "name": "URL Redirector",
  "description": "Redirect url by pattern to any place you want.",
  "version": "1.0",
  "background":{
	"scripts":["js/background.js"]
  },
  "permissions": [
	"webRequest",
    "*://*/*",
	"storage",
	"webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "images/mart.png",
	"default_title": "URL Redirector",
    "default_popup": "index.html"
  },
  "options_page":"option.html"
}
